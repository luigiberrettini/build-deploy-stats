{
    "Zabbix": {
        "enabled": true,
        "server": "127.0.0.1",
        "port": 10051,
        "hostname": "myHostName",
        "discovery_rule_key": "myDiscovery",
        "category_tool_macro": "{#TOOL}",
        "category_context_macro": "{#TYPE}"
    },
    "TeamCity": {
        "enabled": true,
        "page_size": 100,
        "user": "myTCUser",
        "password": "myTCPassword",
        "server_url": "http://tc.domain/",
        "verify_ssl_certs": true,
        "api_url_prefix": "httpAuth/app/rest/",
        "project_id": "MyTCProject"
    },
    "Jenkins": {
        "enabled": false,
        "page_size": 100,
        "user": "myJKUser",
        "password": "myJKPassword",
        "server_url": "http://jk.domain/",
        "verify_ssl_certs": true,
        "api_url_prefix": "jkprefix/",
        "project_id": "MyJKProject"
    },
    "OctopusDeploy": {
        "enabled": true,
        "api_key": "myOPDApiKey",
        "server_url": "http://opd.domain/",
        "verify_ssl_certs": true,
        "api_url_prefix": "api/",
        "project_group_filter": "*MyFilter*"
    },
    "UrbanCodeDeploy": {
        "enabled": false,
        "page_size": 100,
        "user": "myUCDUser",
        "password": "myUCDPassword",
        "server_url": "http://tc.domain/",
        "verify_ssl_certs": true,
        "api_url_prefix": "ucdprefix/",
        "project_id": "MyUCDProject"
    }
}