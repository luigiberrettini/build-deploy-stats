{
    "Zabbix": {
        "enabled": true,
        "server": "127.0.0.1",
        "port": 10051,
        "hostname": "myHostName",
        "discovery_rule_key": "myDiscovery",
        "category_tool_macro": "{#TOOL}",
        "category_context_macro": "{#TYPE}"
    },
    "TeamCity": {
        "enabled": true,
        "page_size": 100,
        "user": "myTCUser",
        "password": "myTCPassword",
        "server_url": "http://tc.domain/",
        "verify_ssl_certs": true,
        "api_url_prefix": "httpAuth/app/rest/",
        "project_id": "MyTCProject"
    },
    "Jenkins": {
        "enabled": false,
        "page_size": 100,
        "user": "myJKUser",
        "password_or_auth_token": "myJKPassword",
        "server_url": "http://jk.domain/",
        "verify_ssl_certs": true,
        "json_api_url_suffix": "/api/json",
        "xml_api_url_suffix": "/api/xml",
        "job_name": "MyJKJob"
    },
    "OctopusDeploy": {
        "enabled": true,
        "api_key": "myOPDApiKey",
        "server_url": "http://opd.domain/",
        "verify_ssl_certs": true,
        "api_url_prefix": "api/",
        "project_group_filter": "*MyFilter*"
    },
    "UrbanCodeDeploy": {
        "enabled": false,
        "page_size": 100,
        "user": "myUCDUser", // PasswordIsAuthToken if using a token
        "password_or_auth_token": "myUCDPassword",
        "server_url": "http://ucd.domain/",
        "verify_ssl_certs": true,
        "api_url_prefix": "rest/deploy/",
        "tag_name": "MyUCDTag"
    }
}